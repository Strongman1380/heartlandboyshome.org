{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "errorPage": "/404.html",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/programs",
      "destination": "/programs.html"
    },
    {
      "source": "/admissions",
      "destination": "/admissions.html"
    },
    {
      "source": "/staff",
      "destination": "/staff.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/referral",
      "destination": "/referral.html"
    },
    {
      "source": "/volunteer",
      "destination": "/volunteer.html"
    },
    {
      "source": "/donate",
      "destination": "/donate.html"
    },
    {
      "source": "/newsletter",
      "destination": "/newsletter.html"
    },
    {
      "source": "/training",
      "destination": "/training.html"
    },
    {
      "source": "/404",
      "destination": "/404.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}